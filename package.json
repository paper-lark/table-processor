{
    "name": "spreadsheets",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@gravity-ui/browserslist-config": "^4.3.0",
        "@gravity-ui/eslint-config": "^3.1.1",
        "@gravity-ui/icons": "^2.9.0",
        "@gravity-ui/prettier-config": "^1.1.0",
        "@gravity-ui/react-data-table": "^1.2.0",
        "@gravity-ui/stylelint-config": "^4.0.1",
        "@gravity-ui/tsconfig": "^1.0.0",
        "@gravity-ui/ui-logger": "^1.1.0",
        "@gravity-ui/uikit": "^6.1.1",
        "@types/node": "^20.11.20",
        "@types/react": "^18.2.60",
        "@types/react-dom": "^18.2.19",
        "bem-cn-lite": "^4.1.0",
        "cra-template-gravity-ui-pure": "1.1.1",
        "eslint": "^8.57.0",
        "husky": "^8.0.3",
        "hyperformula": "^2.6.2",
        "lint-staged": "^14.0.1",
        "prettier": "^3.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "sass": "^1.71.1",
        "sass-loader": "^13.3.3",
        "stylelint": "^15.11.0",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install",
        "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
        "lint:styles": "stylelint '{styles,src}/**/*.scss'",
        "lint:prettier": "prettier --check '**/*.md'",
        "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        "extends @gravity-ui/browserslist-config"
    ],
    "lint-staged": {
        "*.{scss}": [
            "stylelint --fix --quiet"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --quiet",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18"
    },
    "overrides": {
        "typescript": "^5.3.3"
    }
}
